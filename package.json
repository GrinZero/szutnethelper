{
  "name": "szutnethelper",
  "version": "0.1.0",
  "scripts": {
    "reset": "npx rimraf ./**/node_modules",
    "dev": "modern dev electron",
    "build": "modern build electron",
    "start": "modern start",
    "new": "modern new",
    "lint": "modern lint",
    "dev:brower": "modern dev",
    "build:brower": "modern build",
    "test": "modern test",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=14.17.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "node --max_old_space_size=8192 ./node_modules/eslint/bin/eslint.js --fix --color --cache --quiet"
    ],
    "*.{js,jsx,mjs,mjsx,cjs,cjsx}": [
      "node --max_old_space_size=8192 ./node_modules/eslint/bin/eslint.js --fix --color --cache --quiet"
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "dist/"
  ],
  "dependencies": {
    "@modern-js/runtime": "^1.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "tailwindcss": "^2.2.19",
    "@modern-js/plugin-sass": "^1.3.0"
  },
  "devDependencies": {
    "@babel/register": "^7.15.3",
    "@babel/runtime": "^7.15.4",
    "@modern-js/app-tools": "^1.0.0",
    "@modern-js/plugin-electron": "^1.0.0",
    "@modern-js/plugin-jarvis": "^1.0.0",
    "@modern-js/tsconfig": "^1.0.0",
    "@types/node": "^14",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "electron": "^13.6.9",
    "electron-builder": "^22.11.7",
    "husky": "4.3.8",
    "lint-staged": "^11.2.6",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "typescript": "^4",
    "@modern-js/plugin-tailwindcss": "^1.2.10",
    "@modern-js/plugin-testing": "^1.5.5",
    "@types/jest": "^26.0.9"
  },
  "modernConfig": {},
  "main": "./electron/main.js"
}
